<h1>New Book</h1>
<%= form_for [@book] do |f| %>
    <p><%= f.label :title %><br/>
    <%= f.text_field :title %></p>
    <p><%= f.label :author %><br/>
    <%= f.text_field :authors %></p>
    <p><%= f.label :pages %><br/>
    <%= f.text_field :pages %></p>
    <p><%= f.label :year_published %><br/>
    <%= f.text_field :year_published %></p>
    <p><%= f.label :book_cover %><br/>
    <%= f.text_field :book_cover %></p>
    <p><%= f.submit %></p>
  <% end %>
